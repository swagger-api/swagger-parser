{
  "openapi": "3.0.0",
  "info": {
    "title": "AWS Amplify",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "http://api.ex.com"
    }
  ],
  "paths": {
    "/apps": {
      "post": {
        "operationId": "CreateApp",
        "description": " Creates a new Amplify app. ",
        "responses": {
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "customRules": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/CustomRule"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CustomRule": {
        "type": "object",
        "properties": {
          "source": {
            "type": "string"
          }
        }
      },
      "CustomRules": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/CustomRule"
        }
      }
    }
  }
}
